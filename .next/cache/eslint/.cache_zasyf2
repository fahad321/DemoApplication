],"/home/fshafiq/Desktop/Next-Scalable/nextjs-fullstack-app-template/components/navigation/header/Header.tsx",[],[],"/home/fshafiq/Desktop/Next-Scalable/nextjs-fullstack-app-template/components/templates/base/BaseTemplate.mocks.ts",[],[],"/home/fshafiq/Desktop/Next-Scalable/nextjs-fullstack-app-template/components/templates/base/BaseTemplate.stories.tsx",[],[],"/home/fshafiq/Desktop/Next-Scalable/nextjs-fullstack-app-template/components/templates/base/BaseTemplate.tsx",[],[],"/home/fshafiq/Desktop/Next-Scalable/nextjs-fullstack-app-template/components/users/users.tsx",["173"],[],"import { useEffect, useState } from 'react';\nimport { useDispatch, useSelector } from 'react-redux';\nimport { deleteUser, getUsers, updateUser } from '../../store/reducers/userReducer';\nimport styles from \"./users.module.css\";\n\ninterface User {\n  avatar: string;\n  id: number;\n  first_name: string;\n  last_name: string;\n  email: string;\n}\n\nlet Userslist: User[];\n\nconst Users: React.FC = (): JSX.Element => {\n  const [status, setStatus] = useState<string>('');\n  const [isEdit, setIsEdit] = useState(false);\n  const [firstName, setFirstName] = useState('');\n  const [lastName, setLastName] = useState('');\n  const dispatch = useDispatch();\n  // @ts-ignore\n  const posts = useSelector((state) => state.users.users);\n  const [fList, setFList] = useState(Userslist);\n  const [list, setList] = useState(Userslist);\n\n  useEffect(() => {\n    setFList(posts);\n    setList(posts);\n  }, [posts]);\n\n  function fetchUsers() {\n    dispatch(getUsers());\n  }\n\n  function deleteSelectedUser(user: User) {\n    let userId: number = user.id;\n    const name = user.first_name;\n    dispatch(deleteUser(userId));\n    setStatus(`${name} has been deleted successfully`);\n    setTimeout(() => {\n      setStatus('');\n    }, 3000);\n  }\n\n  function filterSelectedUser(user: User) {\n    for (let i = 0; i < fList.length; i++) {\n      if (user.id === fList[i].id) {\n        let a = [];\n        a.push(fList[i]);\n        setList(a);\n      }\n    }\n  }\n\n  function editSelectedUser(user: User) {\n    setIsEdit(true);\n    for (let i = 0; i < list.length; i++) {\n      if (user.id === list[i].id) {\n        let a = [];\n        a.push(fList[i]);\n        setList(a);\n      }\n    }\n  }\n\n  function saveSelectedUser(user: User) {\n    setIsEdit(false);\n    let updateUser1 = {};\n    const name = user.first_name;\n    Object.assign(updateUser1, {\n      avatar: user.avatar,\n      email: user.email,\n      first_name: firstName,\n      id: user.id,\n      last_name: lastName,\n    });\n    // @ts-ignore\n    dispatch(updateUser(updateUser1));\n    setStatus(`${name} has been updated successfully to ${firstName}`);\n    setTimeout(() => {\n      setStatus('');\n    }, 3000);\n  }\n  return (\n    <div className={styles.App}>\n    <h2>Users from API:</h2>\n    {fList !== undefined && !!fList.length && (\n      <nav className={styles.nav__bar}>\n        {' '}\n        {fList.map((item) => (\n          <li\n            className={styles.nav__bar_links}\n            onClick={() => filterSelectedUser(item)}\n            key={item.id}\n          >\n            {' '}\n            {item.first_name + ' ' + item.last_name}{' '}\n          </li>\n        ))}{' '}\n      </nav>\n    )}\n    <button className={styles.fetch_users} onClick={fetchUsers}>\n      Fetch Users\n    </button>\n    <div>\n      <ul className={styles.list_users}>\n        {' '}\n        {list !== undefined &&\n          !!list.length &&\n          list.map((item) => (\n            <li className={styles.li_user} key={item.id}>\n              {' '}\n              {item.first_name + ' ' + item.last_name}\n              <button onClick={() => editSelectedUser(item)} className={styles.edit}>\n                Edit\n              </button>\n              <button\n                onClick={() => deleteSelectedUser(item)}\n                className={styles.delete}\n              >\n                Delete\n              </button>\n              {!!isEdit && (\n                <div>\n                  <label>First Name</label>\n                  <input\n                    type=\"text\"\n                    className={styles.task}\n                    onChange={(e) => setFirstName(e.target.value)}\n                    name=\"firstName\"\n                  />\n                  <label>Last Name</label>\n                  <input\n                    type=\"text\"\n                    className={styles.task}\n                    onChange={(e) => setLastName(e.target.value)}\n                    name=\"lastName\"\n                  />\n                  <button\n                    onClick={() => saveSelectedUser(item)}\n                    className={styles.delete}\n                  >\n                    Save\n                  </button>\n                </div>\n              )}{' '}\n            </li>\n          ))}{' '}\n      </ul>\n    </div>\n    <p> {status}</p>\n  </div>\n  );\n};\n\nexport default Users;\n","/home/fshafiq/Desktop/Next-Scalable/nextjs-fullstack-app-template/components/utility/search/Search.mocks.ts",[],[],"/home/fshafiq/Desktop/Next-Scalable/nextjs-fullstack-app-template/components/utility/search/Search.stories.tsx",[],[],"/home/fshafiq/Desktop/Next-Scalable/nextjs-fullstack-app-template/components/utility/search/Search.tsx",[],[],"/home/fshafiq/Desktop/Next-Scalable/nextjs-fullstack-app-template/components/utility/search-result/SearchResult.mocks.ts",[],[],"/home/fshafiq/Desktop/Next-Scalable/nextjs-fullstack-app-template/components/utility/search-result/SearchResult.stories.tsx",[],[],"/home/fshafiq/Desktop/Next-Scalable/nextjs-fullstack-app-template/components/utility/search-result/SearchResult.tsx",[],[],"/home/fshafiq/Desktop/Next-Scalable/nextjs-fullstack-app-template/lib/search/types.ts",[],[],{"ruleId":"174","severity":2,"message":"175","line":16,"column":29,"nodeType":"176","messageId":"177","endLine":16,"endColumn":32},"no-undef","